    <!-- http://www.acplt.de/Capability#Absorbing -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Absorbing">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasTGL25000Code"/>
                <owl:hasValue>1-91</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Combining"/>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Separating"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Liquid"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Liquid"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Solid"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Liquid"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Solid"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="de">Absorbieren</rdfs:label>
        <rdfs:label xml:lang="en">Absorbing</rdfs:label>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Acoustic_gas_cleaning -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Acoustic_gas_cleaning">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasTGL25000Code"/>
                <owl:hasValue>1-84</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Briquetting"/>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Separating"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                                <owl:onClass rdf:resource="http://www.acplt.de/Capability#Solid"/>
                            </owl:Restriction>
                        </owl:intersectionOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                    </owl:Restriction>
                </owl:unionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Acoustic gas cleaning</rdfs:label>
        <rdfs:label xml:lang="de">Akustisches Gasreinigen</rdfs:label>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Actuator -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Actuator">
