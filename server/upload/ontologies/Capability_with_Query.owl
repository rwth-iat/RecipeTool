    <!-- http://www.acplt.de/Capability#Agglomerating -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Agglomerating">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#OperationChangingMaterial"/>
        <rdfs:comment>TGL 25000 4-01</rdfs:comment>
        <rdfs:label xml:lang="en">Agglomerating</rdfs:label>
        <rdfs:label xml:lang="de">Agglomerieren</rdfs:label>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Agitator -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Agitator">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Actuator"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://www.w3id.org/basyx/c4i#hasCapability"/>
                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Stirring"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#AnalogValve -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#AnalogValve">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Valve"/>
        <owl:disjointWith rdf:resource="http://www.acplt.de/Capability#On/OffValve"/>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Atomizing -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Atomizing">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasTGL25000Code"/>
                <owl:hasValue>2-50</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Combining"/>
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#usesEduct"/>
                        <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Solid"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.basys40.de/kb/c4i.owl#producesProduct"/>
                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Gas"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#canBeRealisedBy"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.acplt.de/Capability#Dosing"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Atomizing</rdfs:label>
        <rdfs:label xml:lang="de">Verstäuben</rdfs:label>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#BarrelPump -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#BarrelPump">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Pump"/>
        <rdfs:comment xml:lang="de">Fasspumpe</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Beading -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Beading">
        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasTGL25000Code"/>
                <owl:hasValue>3-20</owl:hasValue>
            </owl:Restriction>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#Dividing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#canBeRealisedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Detach"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label xml:lang="en">Beading</rdfs:label>
        <rdfs:label xml:lang="de">Verperlen</rdfs:label>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#BranchL -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#BranchL">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#ControlledValve"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasSubSystem"/>
                <owl:someValuesFrom rdf:resource="http://www.acplt.de/Capability#Pump"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.acplt.de/Capability#hasSubSystem"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.acplt.de/Capability#Valve"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.acplt.de/Capability#Branching -->

    <owl:Class rdf:about="http://www.acplt.de/Capability#Branching">
        <rdfs:subClassOf rdf:resource="http://www.acplt.de/Capability#ProcessSpecificCapability"/>
        <rdfs:comment xml:lang="en">some trick is needed here:
because this process specific capability needs at least 2 valves and it cannot be defined by &apos;associatedWithCapability min 2 FlowStoping&apos;.
--&gt; so far the restriction is defined on the &apos;hasSubSystem min 2 Valve&apos;.
--&gt; waiting for further solution.</rdfs:comment>
    </owl:Class>
    


